{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --entry 'src/**/*.ts'",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "ts-parser-combinators",
  "author": "Angus Trau",
  "module": "dist/ts-parser-combinators.esm.js",
  "size-limit": [
    {
      "path": "dist/ts-parser-combinators.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ts-parser-combinators.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "husky": "^4.3.6",
    "size-limit": "^4.9.1",
    "tsdx": "https://github.com/agilgur5/tsdx.git#dist-multi",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
